   "source": [
    "import openpyxl\n",
    "\n",
    "# Tạo workbook mới\n",
    "wb = openpyxl.Workbook()\n",
    "ws = wb.active\n",
    "ws.title = \"AllowedSubjects\"\n",
    "\n",
    "# Ghi header\n",
    "ws.append([\"id\", \"subject\"])\n",
    "\n",
    "# Ghi một dòng dữ liệu\n",
    "ws.append([1, \"NỘP CV\"])\n",
    "\n",
    "# Lưu file\n",
    "file_path = \"allowed_subjects.xlsx\"\n",
    "wb.save(file_path)\n",
    "\n",
    "file_path\n"
   ]
